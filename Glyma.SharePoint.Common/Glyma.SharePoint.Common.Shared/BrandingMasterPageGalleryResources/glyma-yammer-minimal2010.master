                                                                                                runat="server"
                                                                                                ImageUrl="/_layouts/images/fgimg.png"
                                                                                                Text="<%$Resources:wss,multipages_launchdevdashalt_text%>"
                                                                                                OffsetX=0
                                                                                                OffsetY=222
                                                                                                Height=16
                                                                                                Width=16 />
                                                                                </span>
                                                                </span>
                                                </SharePoint:SPRibbonPeripheralContent>
                  </SharePoint:SPRibbon>
                  
                  <!-- end main ribbon control -->
                </div>
    </div>
</asp:Panel>

