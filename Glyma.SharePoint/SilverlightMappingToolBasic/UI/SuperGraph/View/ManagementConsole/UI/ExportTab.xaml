                        <telerik:RadComboBox telerik:StyleManager.Theme="Windows8" Text="Select Export Type" Width="150" Height="30" Margin="0,5,10,5" x:Name="ExportTypeBox" 
                                         SelectedValue="{Binding Path=ExportType, Mode=TwoWay}" SelectionChanged="ExportTypeBox_OnSelectionChanged" ToolTipService.ToolTip="Select the format to export to">
